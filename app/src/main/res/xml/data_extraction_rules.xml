<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude audio files and generated images from backup -->
        <exclude domain="file" path="audio/" />
        <exclude domain="file" path="generated/" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow user data transfer between devices -->
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>